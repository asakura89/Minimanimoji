<UserControl x:Class="Minimanimoji.MojiPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Minimanimoji"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel x:Name="MojiStackPanel" Orientation="Vertical">
            <TextBlock x:Name="TitleBlock" HorizontalAlignment="Stretch" Text="{Binding Path=MojiTitle}" FontSize="24"></TextBlock>
        </StackPanel>
    </Grid>
</UserControl>
